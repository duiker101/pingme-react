{
  "name": "pingme-react",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://ping.riftkit.net/",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-loader-spinner": "^2.1.0",
    "react-scripts": "1.1.4",
    "sass": "^1.12.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "add-cname": "echo ping.riftkit.net > docs/CNAME",
    "move-build": "rm -rf docs && mv build docs && mv storybook-static docs/storybook",
    "build": "react-scripts build && yarn run build-storybook && yarn run move-build && yarn run add-cname",
    "test": "react-scripts test --env=jsdom",
    "sass": "sass src:src --watch",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public "
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "sinon": "^6.1.5"
  }
}
